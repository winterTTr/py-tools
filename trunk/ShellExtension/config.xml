<?xml version='1.0' encoding='utf-8' ?>
<config>
	<menu>
		<name>open cmd here...</name>
		<match>
            <number>1</number>
			<type>dir</type>
		</match>
		<command>
            <exec>cmd.exe /K "cd /d %s"</exec>
		</command>
	</menu>
    <menu>
        <name>send to localhost:8787 ...</name>
        <match>
            <number>1</number>
            <type>file</type>
            <pattern>(\.json|\.gsl)$</pattern>
        </match>
        <command>
            <exec>cmd.exe /K "cd /d %s"</exec>
        </command>
    </menu>
	<menu>
		<name>open with &amp;gvim...</name>
		<match>
			<num>-1</num>
			<type>file,dir</type>
		</match>
		<command>
			<exec>gvim.exe -p %s</exec>
		</command>
	</menu>
	<menu>
		<name>Open Target Dir...</name>
		<match>
			<num>1</num>
			<type>link</type>
		</match>
		<command>
			<exec>explorer /e,/select,%s</exec>
		</command>
	</menu>
</config>
